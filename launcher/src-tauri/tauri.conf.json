{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "retreivr-launcher",
  "version": "0.1.0",
  "identifier": "com.logan.retreivr-launcher",
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "retreivr-launcher",
        "width": 800,
        "height": 600,
        "url": "index.html",
        "devtools": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": ["app", "dmg", "nsis", "msi"]
  }
}
